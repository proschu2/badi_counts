{
  "hosting": {
    "public": "build/web",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "headers": [
      {
        "source": "/favicon.ico",
        "headers": [
          {
            "key": "Cache-Control",
            "value": "max-age=3600"
          }
        ]
      }
    ],
    "rewrites": [
      {
        "source": "**",
        "destination": "/index.html"
      }
    ]
  },
  "config": {
    "flutter": {
      "platforms": {
        "android": {
          "default": {
            "projectId": "badi-counts-zh",
            "appId": "1:1097937205336:android:f3a66d8338db9f2e42b22d",
            "fileOutput": "android/app/google-services.json"
          }
        },
        "dart": {
          "lib/firebase_options.dart": {
            "projectId": "badi-counts-zh",
            "configurations": {
              "android": "1:1097937205336:android:f3a66d8338db9f2e42b22d",
              "web": "1:1097937205336:web:755a7d496e7354fb42b22d"
            }
          }
        }
      }
    }
  }
}